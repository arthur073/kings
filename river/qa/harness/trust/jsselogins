	keyStoreAlias="reggie"
	keyStoreURL="jar:file:${com.sun.jini.qa.harness.harnessJar}!/harness/trust/reggie.keystore"
	keyStorePasswordURL="jar:file:${com.sun.jini.qa.harness.harnessJar}!/harness/trust/reggie.password";
};

